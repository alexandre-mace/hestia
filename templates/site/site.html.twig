{% extends 'back-office.html.twig' %}

{% block content %}
    {{ site.name }}
    {% for siteMaterial in site.siteMaterials %}
        {{ siteMaterial.material.name }} - {{ siteMaterial.quantity }}
    {% endfor %}
    {{ site.recyclingRate }}
    {% if site.label is defined and site.label is not null %}
        {{ site.label.grade }}
    {% endif %}
    {{ site.manager.username }}
    <br>
{% endblock %}
